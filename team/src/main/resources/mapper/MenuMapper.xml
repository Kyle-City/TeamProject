<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.team.mapper.MenuMapper">
    <insert id="addRelation">
        INSERT INTO menu_food (menu_id,ingredient_id)values
        <foreach collection="foodId" item="foodId" separator=",">
            (#{menuId}, #{foodId})
        </foreach>


    </insert>
</mapper>